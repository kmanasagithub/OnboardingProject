<h2>Search Results for Tag: <%= @tag %></h2>
<div id="images">
<% if @search_results.present? %>
  
    <% @search_results.each do |image| %>

   
   <%= link_to image_tag(image.image, alt: "Image", style: "width: 430px; height: 300px;"), image %>
           
   <% end %>
  
<% else %>
  <p>No results found for tag: <%= @tag %></p>
<% end %>
</div>
